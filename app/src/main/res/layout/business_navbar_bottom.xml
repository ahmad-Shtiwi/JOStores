<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="0dp"
        android:background="@color/master">

        <ImageButton
            android:id="@+id/profile_button_id"
            style="@style/navbar_bottom_button"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="10dp"
            app:srcCompat="@drawable/account_icon"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/add_item_button_id"

            style="@style/navbar_bottom_button"
            android:layout_marginEnd="15dp"

            android:layout_toStartOf="@+id/chat_button_id"
            app:srcCompat="@drawable/add_item_icon" />

        <ImageButton
            android:id="@+id/chat_button_id"
            style="@style/navbar_bottom_button"
            android:layout_marginEnd="15dp"
            android:layout_toStartOf="@+id/profile_button_id"
            app:srcCompat="@drawable/chat_icon" />

        <ImageButton
            android:id="@+id/home_button_id"
            style="@style/navbar_bottom_button"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="1dp"
            android:layout_toStartOf="@+id/search_button_id"
            app:srcCompat="@drawable/home_icon" />

        <ImageButton
            android:id="@+id/search_button_id"
            style="@style/navbar_bottom_button"

            android:layout_marginEnd="15dp"
            android:layout_toStartOf="@+id/add_item_button_id"

            app:srcCompat="@drawable/search_icon" />
    </RelativeLayout>


</RelativeLayout>